<h1>Destinations I have travelled too !</h1>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>
<% if @current_user %>
<p>
  <a href="/places/new">New place</a>
</p>  
<% else %>
  You must first <a href="/login">login</a> to post.
<% end %>




